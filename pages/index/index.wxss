/**index.wxss**/
.index-container{
  position:relative;
  width:100%;
  height:100vh;
  padding-bottom:120rpx;
  box-sizing:border-box;
}


.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.container .calendar-wrap{
  margin-top:20rpx;
  margin-bottom:20rpx;
}

.container .swipe{
  width: 686rpx;
  height: 110rpx;
  background-color: #ff9214;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 0 32rpx;
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  font-weight: bold;
}
